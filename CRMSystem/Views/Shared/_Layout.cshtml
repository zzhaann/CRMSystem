@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer localizer
<!DOCTYPE html>
<html data-wf-page="5d2f26e24904ea05c16c0fad" data-wf-site="5d2f26e24904ea2ed96c0fac" data-wf-status="1">
<head>
    <meta charset="utf-8" />
    <title>@localizer["Title"].Value.ToString()</title>
    <meta content="Artboard - is a classic Admin/Dashboard template and UI kit which is responsive, feature rich and easily customizable." name="description" />
    <meta content="Artboard - Webflow HTML website template" property="og:title" />
    <meta content="Artboard - is a classic Admin/Dashboard template and UI kit which is responsive, feature rich and easily customizable." property="og:description" />
    <meta content="images/5f22afd1c6a4c1b0e4998fad_og.jpg" property="og:image" />
    <meta content="Artboard - Webflow HTML website template" property="twitter:title" />
    <meta content="Artboard - is a classic Admin/Dashboard template and UI kit which is responsive, feature rich and easily customizable." property="twitter:description" />
    <meta content="images/5f22afd1c6a4c1b0e4998fad_og.jpg" property="twitter:image" />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Webflow" name="generator" />
    <link href="~/css/artboard.webflow.80ed7f466.min.css" rel="stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">
        WebFont.load({
            google: { families: ["DM Sans:regular,500,700"] }
        });
    </script>
    <link href="~/images/favicon.png" rel="shortcut icon" type="image/x-icon" />
    <link href="~/images/app-icon.png" rel="apple-touch-icon" />
    <link href="~/css/artboard.webflow.80ed7f466.min.css" rel="stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">
        WebFont.load({
            google: {
                families: ["DM Sans:regular,500,700"]
            }
        });
    </script>
    <!--[if lt IE 9]>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script>
    <![endif]-->
    <script type="text/javascript">
        !function(o, c) {
            var n = c.documentElement,
                t = " w-mod-";
            n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
        }(window, document);
    </script>
    <link href="~/images/favicon.png" rel="shortcut icon" type="image/x-icon" />
    <link href="~/images/app-icon.png" rel="apple-touch-icon" />

    <!-- Подключение Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">


</head>
<body class="body">
    <div class="app-layout">

        <div class="app-navbar-container">
            <div class="app-navbar-menu">
                <!-- Поиск -->
                <div class="app-navbar-menu-item">
                    <div data-delay="0" class="menu-dropdown w-dropdown">
                        <div class="menu-dropdown-toggle dropdown-search w-dropdown-toggle"></div>
                        <nav class="menu-dropdown-list search search-dropdown-list w-dropdown-list">
                            <form action="/search" class="search w-form">
                                <input type="search" class="search-input w-input" maxlength="256" name="query" placeholder="Search…" id="search" required="" />
                                <input type="submit" value="Search" class="search-button w-button" />
                            </form>
                        </nav>
                    </div>
                </div>
                <!-- Inbox (входящие сообщения) -->
                <div class="app-navbar-menu-item">
                    <div data-delay="0" class="menu-dropdown w-dropdown">
                        <div class="menu-dropdown-toggle dropdown-message w-dropdown-toggle"></div>
                        <nav class="menu-dropdown-list w-dropdown-list">
                            <div class="notification-content-heading"><div>Inbox (6)</div></div>
                            <div class="notification-content-body">
                                <a href="#" class="notification-item w-inline-block">
                                    <div class="notification-item-icon _1"></div>
                                    <div class="notification-item-body">
                                        <div class="notification-item-title">Anny Sanders</div>
                                        <div class="notification-item-text">This is some text inside of a div block.</div>
                                    </div>
                                    <div class="notification-item-date">1h ago</div>
                                </a>
                                <a href="/inbox" class="custom-link">View all</a>
                            </div>
                        </nav>
                    </div>
                </div>

                <!-- Уведомления -->
                <div class="app-navbar-menu-item">
                    <div data-delay="0" class="menu-dropdown w-dropdown">
                        <div class="menu-dropdown-toggle dropdown-notification w-dropdown-toggle"></div>
                        <nav class="menu-dropdown-list w-dropdown-list">
                            <div data-duration-in="300" data-duration-out="100" class="notification-tabs w-tabs">
                                <div class="tabs-menu notification-menu w-tab-menu">
                                    <a data-w-tab="Tab 1" class="tab-link notification-tab w-inline-block w-tab-link">
                                        <img src="images/5e906cca4576bc78b0b5afcf_icons8-notification-center-100.png" width="25" alt="" class="notification-icon" />
                                        <div class="hint">Notifications</div>
                                    </a>
                                </div>
                                <div class="tabs-content notification-content w-tab-content">
                                    <div data-w-tab="Tab 1" class="w-tab-pane">
                                        <div class="notification-content-heading"><div>Notifications (3)</div></div>
                                        <div class="notification-content-body">
                                            <a href="#" class="notification-item w-inline-block">
                                                <div class="notification-item-icon _4"></div>
                                                <div class="notification-item-body">
                                                    <div class="hint"><span class="hint-bold">Izabella Watson</span> liked your post.</div>
                                                    <div class="notification-item-time">2h ago</div>
                                                </div>
                                            </a>
                                            <a href="/inbox" class="custom-link">View all</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>

                <!-- Профиль -->
                <div class="app-navbar-menu-item">
                    <div data-delay="0" class="menu-dropdown w-dropdown">
                        <div class="menu-dropdown-toggle w-dropdown-toggle">
                            <div class="navbar-dropdown-avatar"></div>
                        </div>
                        <nav class="menu-dropdown-list w-dropdown-list">
                            <a href="/user-profile" class="menu-dropdown-link w-inline-block">
                                <div class="munu-dropdown-link-container">
                                    <img src="images/5e8a0ba64fbaead7c8227773_icons8-male-user-100-20-3-.png" width="20" alt="" class="menu-dropdown-link-icon" />
                                    <div>Profile</div>
                                </div>
                            </a>
                            <a href="/account-settings/general" class="menu-dropdown-link w-inline-block">
                                <div class="munu-dropdown-link-container">
                                    <img src="images/5e8a0bc6f99d7c01a2738435_icons8-settings-100-20-2-.png" width="20" alt="" class="menu-dropdown-link-icon" />
                                    <div>Settings</div>
                                </div>
                            </a>
                            <a href="/account/logout" class="menu-dropdown-link w-inline-block">
                                <div class="munu-dropdown-link-container">
                                    <img src="images/5e8a0c2aa6b2c1d212a8243c_icons8-logout-rounded-left-100-20-1-.png" width="20" alt="" class="menu-dropdown-link-icon" />
                                    <div>Logout</div>
                                </div>
                            </a>
                        </nav>
                    </div>
                </div>

            </div>
        </div>



        <!--MENU-->
        <div class="app-nav-layout">
            <div data-collapse="medium" data-animation="over-left" data-duration="400" data-easing="ease-in-out" data-easing2="ease-in-out" role="banner" class="sidebar-navbar w-nav">
                <div class="sidebar-navbar-container w-container">
                    <div class="sidebar-navbar-heading">
                        <div class="sidebar-navbar-menu-button w-nav-button"></div>
                        <a href="/" aria-current="page" class="sidebar-navbar-brand w-nav-brand w--current">
                            <!-- <img src="~/images/5e89ee4106f670eaf2d24514_artboard-20white-20color.svg" height="20" alt="" class="sidebar-navbar-brand-logo" />-->
                            <img src="~/images/5e89f543bcf6e14d29c7914e_artboard-20color-20small.svg" height="20" alt="" class="sidebar-navbar-brand-logo-small" />
                        </a>
                    </div>
                    <nav role="navigation" class="sidebar-navbar-menu w-nav-menu">
                        <div class="sidebar-navbar-body">
                            <ul class="sidebar-navbar-body-container">
                                <li class="sidebar-navbar-link link-home w-nav-link w--current" style="color: white; text-decoration: none;">
                                    @Html.ActionLink("Home", "Index", "Home", null, new { style = "color: inherit; text-decoration: none;" })
                                </li>
                                <li class="sidebar-navbar-link w-nav-link" style="color: white; text-decoration: none;">
                                    @Html.ActionLink("Dachboard", "Incoming", "Dashboard", null, new { style = "color: inherit; text-decoration: none;" })
                                </li>
                                <li class="sidebar-navbar-link link-setting w-nav-link" style="color: white; text-decoration: none;">
                                    @Html.ActionLink("Account Settings", "Profile", "User", null, new { style = "color: inherit; text-decoration: none;" })
                                </li>
                                <li class="sidebar-navbar-link link-payment w-nav-link" style="color: white; text-decoration: none;">
                                    @Html.ActionLink("Contracts", "Index", "Contracts", null, new { style = "color: inherit; text-decoration: none;" })
                                </li>
                                <li class="sidebar-navbar-link link-inbox w-nav-link" style="color: white; text-decoration: none;">
                                    @Html.ActionLink("Inbox", "Index", "Inbox", null, new { style = "color: inherit; text-decoration: none;" })
                                </li>
                                <li class="sidebar-navbar-link link-projects w-nav-link" style="color: white; text-decoration: none;">
                                    @Html.ActionLink("Analytics", "AnalyzeDay", "Analytics", null, new { style = "color: inherit; text-decoration: none;" })
                                </li>
                            </ul>
                        </div>
                        <div class="sidebar-navbar-footer">
                            <div class="sidebar-navbar-link" >@Html.Partial("_Langs")</div>
                            <a href="/style-guide" class="sidebar-navbar-link link-help w-nav-link">Help</a>
                            <a href="/account/logout" class="sidebar-navbar-link link-logout w-nav-link">Logout</a>
                        </div>
                        <div class="sidebar-navbar-footer nav-toggle">
                            <a data-w-id="c953acc7-394f-2eba-e671-0dd464d91c76" href="#" class="sidebar-navbar-toggle-button w-button"></a>
                        </div>

                    </nav>
                </div>
            </div>
        </div>
       
            @RenderBody()



            <!--FOOTER-->
    
                <div class="app-main-layout-wrapper">
                    <div class="app-main-layout-content">
                        <div class="footer">
                            <div class="w-row">
                                <div class="w-col w-col-6">
                                    <div class="hint">© Copyright Aigyrs. All rights reserved</div>
                                </div>
                                <div class="footer-right w-col w-col-6">
                                    <div class="hint">
                                        <a href="https://webflow.com/" class="footer-link">Powered by Aigyrs</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
         
                  </div>
                  </div>


    <script src="~/js/jquery.js" type="text/javascript"></script>
    <script src="~/js/webflow-script.js" type="text/javascript"></script>
        <script src="~/js/jquery.js" type="text/javascript"></script>
        <script src="~/js/webflow-script.js" type="text/javascript"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        function ChangeCulture(culture){
            $.ajax({
                url: '/Home/ChangeCulture',
                data: {culture: culture},
                dataType: 'json',
                method: 'POST',
                success: function(){
                    location.reload();
                }
            })
        }
    </script>
</body>
</html>
